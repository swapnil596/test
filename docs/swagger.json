{
    "swagger": "2.0",
    "info": {
        "description": "A service where the user can register it's own pre-built APIs to eventually consume in Workflow Designer \u0026 Journey Designer",
        "title": "FlowXpert API Framer",
        "termsOfService": "https://swagger.io/terms/",
        "contact": {
            "name": "Neil Haria",
            "url": "https://www.swagger.io/support",
            "email": "neil.haria@think360.ai"
        },
        "version": "2.0"
    },
    "host": "localhost:8008",
    "basePath": "/api/v1/",
    "paths": {
        "/frame": {
            "post": {
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Registration"
                ],
                "parameters": [
                    {
                        "description": "info",
                        "name": "createBaseFrame",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/apis.CreateController"
                        }
                    }
                ]
            }
        },
        "/health": {
            "get": {
                "description": "health check to find the application is up and running",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "HealthCheck"
                ],
                "summary": "get status of the application",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/common.JSONSuccessResult"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "integer"
                                        },
                                        "data": {
                                            "$ref": "#/definitions/models.Health"
                                        },
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        },
        "/{api_id}": {
            "delete": {
                "description": "checks if the API is registered or not, then disables the state of the API if present",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Registration"
                ],
                "summary": "disables the state of API",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/common.JSONSuccessResult"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "code": {
                                            "type": "integer"
                                        },
                                        "data": {
                                            "$ref": "#/definitions/models.APIStatus"
                                        },
                                        "message": {
                                            "type": "string"
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "apis.CreateController": {
            "type": "object",
            "required": [
                "apiType",
                "id",
                "name"
            ],
            "properties": {
                "apiType": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "project_id": {
                    "type": "string"
                },
                "version": {
                    "type": "string"
                }
            }
        },
        "common.JSONSuccessResult": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer",
                    "example": 200
                },
                "data": {
                    "type": "object"
                },
                "message": {
                    "type": "string",
                    "example": "Success"
                }
            }
        },
        "models.APIStatus": {
            "type": "object",
            "properties": {
                "success": {
                    "type": "boolean",
                    "example": true
                }
            }
        },
        "models.Health": {
            "type": "object",
            "properties": {
                "status": {
                    "type": "string",
                    "example": "running"
                }
            }
        }
    }
}